@section Scripts {

    <script>
        $('#btnEnviar').click(function () { 
            data = $('#frmProduto').serialize();
            $.ajax({
                dataType: "json",
                type: "POST",
                url: "/Produto/Create",
                data: data
            }).done(function (result) {
                alert()
              
            }).fail(function (error) {
                console.error(error);
            });

        })
    </script>

    <script>
        var $form = null;
        $(function () {

            $form = $('#fileupload').fileupload({
